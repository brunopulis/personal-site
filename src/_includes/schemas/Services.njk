    "name": "Bruno Pulis",
    "url": "https://brunopulis.com",
    "logo": "https://brunopulis.com/logo.png",
    "sameAs": [
      "https://github.com/brunopulis",
      "https://linkedin.com/in/brunopulis",
      "https://mastodon.social/@brunopulis"
    ],
    "address": {
