    "@astrojs/vercel": "^9.0.1",
    "@cedx/akismet": "^17.1.0",
    "@fontsource-variable/source-sans-3": "^5.2.9",
    "@fontsource/source-sans-3": "^5.2.9",
    "@lucide/astro": "^0.554.0",
    "@philnash/astro-pagination": "^0.5.0",
