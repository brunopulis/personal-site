{
	"name": "personal-site",
	"version": "1.0.0",
	"description": "My personal website",
	"author": "Bruno Pulis",
	"license": "ISC",
	"type": "module",
	"packageManager": "pnpm@10.29.2+sha512.bef43fa759d91fd2da4b319a5a0d13ef7a45bb985a3d7342058470f9d2051a3ba8674e629672654686ef9443ad13a82da2beb9eeb3e0221c87b8154fff9d74b8",
	"scripts": {
		"build:css": "postcss src/assets/css/index.css -o _site/assets/css/index.css --minify",
		"watch:css": "postcss src/assets/css/index.css -o _site/assets/css/index.css --watch",
		"dev": "tinacms dev -c \"concurrently \\\"eleventy --serve --incremental\\\" \\\"pnpm watch:css\\\"\"",
		"dev:full": "tinacms dev -c \"concurrently \\\"eleventy --serve\\\" \\\"pnpm watch:css\\\"\"",
		"build": "SKIP_DB_INDEX_CHECK=true tinacms build --local && eleventy && pnpm build:css",
		"clean": "rimraf _site",
		"clean:all": "pnpm clean && pnpm clean:og",
		"lint": "biome lint .",
		"lint:fix": "biome lint --write .",
		"format": "biome format --write .",
		"check": "biome check --write ."
	},
	"keywords": [],
	"repository": {
		"type": "git",
		"url": "https://github.com/brunopulis/personal-website.git"
	},
	"dependencies": {
		"@11ty/eleventy": "^3.1.2",
		"@11ty/eleventy-fetch": "^5.1.1",
