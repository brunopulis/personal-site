{
  "name": "brunopulis",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "astro": "astro",
    "astro:build": "astro build && pagefind --site dist",
    "dev:tina": "tinacms dev -c",
    "dev": "tinacms dev -c \"astro dev\"",
    "build": "tinacms build && astro build",
    "build:pagefind": "pagefind --site dist",
    "preview": "astro preview",
    "a11y": "pa11y-ci",
    "a11y:report": "node test-a11y.mjs",
    "a11y:dev": "npm run dev & sleep 5 && npm run a11y:report",
    "a11y:build": "npm run build && npm run preview & sleep 5 && npm run a11y:report",
    "start": "astro dev",
    "sync": "astro sync",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
